{
  "name": "@ossinsight/feeds-server",
  "version": "1.0.0",
  "private": true,
  "description": "The feeds server of OSSInsight.",
  "homepage": "https://github.com/pingcap/ossinsight#readme",
  "bugs": {
    "url": "https://github.com/pingcap/ossinsight/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pingcap/ossinsight.git"
  },
  "license": "MIT",
  "main": "app.ts",
  "scripts": {
    "build:ts": "tsc",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -p 30001 -l debug -P dist/app.js",
    "start": "npm run build:ts && fastify start -p 30001 -l info dist/app.js",
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "watch:ts": "tsc -w"
  },
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/env": "^4.1.0",
    "@fastify/sensible": "^5.0.0",
    "@prisma/client": "^4.5.0",
    "async": "^3.2.4",
    "bufferutil": "^4.0.7",
    "canvas": "^2.10.2",
    "dotenv": "^16.0.3",
    "encoding": "^0.1.13",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.5.1",
    "fastify-cron": "^1.3.1",
    "fastify-plugin": "^4.3.0",
    "fastify-prisma-client": "^3.1.1",
    "luxon": "^3.1.0",
    "mailing": "^0.8.12",
    "mailing-core": "^0.8.12",
    "mjml": "^4.13.0",
    "mjml-react": "^2.0.8",
    "next": "^v12.3.2",
    "nodemailer": "^6.8.0",
    "nodemailer-postmark-transport": "^5.2.1",
    "octokit": "^2.0.10",
    "postmark": "^3.0.14",
    "prisma": "^4.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "uglify-js": "^3.17.4",
    "utf-8-validate": "^5.0.10"
  },
  "devDependencies": {
    "@types/async": "^3.2.15",
    "@types/luxon": "^3.0.2",
    "@types/mjml": "^4.7.0",
    "@types/mjml-react": "^2.0.6",
    "@types/node": "^18.0.0",
    "@types/nodemailer": "^6.4.6",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/tap": "^15.0.5",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
