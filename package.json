{
  "name": "gharchive.live-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start --cwd $(pwd) 'pnpm run server:start' --name 'gharchive.live-backend'",
    "stop": "pm2 stop 'gharchive.live-backend'",
    "restart": "pm2 restart 'gharchive.live-backend'",
    "remove": "pm2 delete 'gharchive.live-backend'",
    "postinstall": "pnpm run build",
    "build": "tsc",
    "server:start": "node index.js",
    "dev": "ts-node index.ts",
    "gen:schema-ts": "json2ts params.schema.json params.schema.d.ts",
    "validate:params": "ajv -s params.schema.json -d \"queries/*/params.json\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "consola": "^2.15.3",
    "dotenv": "^16.0.0",
    "generic-pool": "^3.8.2",
    "koa": "^2.13.4",
    "koa-router": "^10.1.1",
    "luxon": "^2.3.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "octokit": "^1.7.1"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.1.1",
    "@types/luxon": "^2.0.9",
    "@types/mysql": "^2.15.21",
    "@types/node": "^17.0.18",
    "ajv-cli": "^5.0.0",
    "json-schema-to-typescript": "^10.1.5",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
