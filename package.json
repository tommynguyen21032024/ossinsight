{
  "name": "gharchive.live-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start --cwd $(pwd) 'pnpm run server:start' --name 'gharchive.live-api'",
    "stop": "pm2 stop 'gharchive.live-api'",
    "restart": "pm2 restart 'gharchive.live-api'",
    "remove": "pm2 delete 'gharchive.live-api'",
    "prefetch:start": "pm2 start --cwd $(pwd) 'pnpm run server:prefetch' --name 'gharchive.live-api-prefetch'",
    "prefetch:stop": "pm2 stop 'gharchive.live-api-prefetch'",
    "prefetch:restart": "pm2 restart 'gharchive.live-api-prefetch'",
    "prefetch:remove": "pm2 delete 'gharchive.live-api-prefetch'",
    "gc:start": "pm2 start --cwd $(pwd) 'pnpm run server:gc' --name 'gharchive.live-api-cache-gc'",
    "gc:stop": "pm2 stop 'gharchive.live-api-cache-gc'",
    "gc:restart": "pm2 restart 'gharchive.live-api-cache-gc'",
    "gc:remove": "pm2 delete 'gharchive.live-api-cache-gc'",
    "postinstall": "pnpm run build",
    "build": "tsc",
    "server:start": "node index.js",
    "server:prefetch": "node compute-prefetch-enums.js",
    "server:gc": "node cache-gc.js",
    "dev": "tsc && ts-node index.ts",
    "prefetch": "tsc && ts-node compute-prefetch-enums.js",
    "generate-company-name-sql": "tsc && ts-node generate-company-name-sql.js",
    "gen:schema-ts": "json2ts params.schema.json params.schema.d.ts",
    "validate:params": "ajv -s params.schema.json -d \"queries/*/params.json\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "consola": "^2.15.3",
    "dotenv": "^16.0.0",
    "generic-pool": "^3.8.2",
    "koa": "^2.13.4",
    "koa-router": "^10.1.1",
    "koa2-ratelimit": "^1.1.0",
    "luxon": "^2.3.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "octokit": "^1.7.1",
    "prom-client": "^14.0.1",
    "redis": "^4.0.4",
    "tiny-async-pool": "^1.2.0",
    "yaml": "^2.0.1"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.1.1",
    "@types/luxon": "^2.0.9",
    "@types/mysql": "^2.15.21",
    "@types/node": "^17.0.18",
    "@types/tiny-async-pool": "^1.0.0",
    "ajv-cli": "^5.0.0",
    "json-schema-to-typescript": "^10.1.5",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
